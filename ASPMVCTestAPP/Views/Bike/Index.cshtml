@model List<Bike>


@{
}
<div class="text-center">
    <h1 class="display-4">Welcome to Bike</h1>
    <table>
        <thead>
            <tr>
                <td>Bike ID</td>
                <td>Name</td>
                <td>Max Speed</td>
                <td>Model</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var bike in Model)
            {
                <tr>
                    <td>@bike.Id</td>
                    <td>@bike.Name</td>
                    <td>@bike.MaxSpeed</td>
                    <td>@bike.Model</td>
                </tr>
            }
        </tbody>
    </table>
</div>